{
    "env": {
        "browser": true,
        "es6": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "google"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "no-undef": "off",
        "no-invalid-this": "off",
        "max-len": "off",
        "indent": ["error", 4],
        "new-cap": "off",
        "no-unused-vars": "off"
    }
}
