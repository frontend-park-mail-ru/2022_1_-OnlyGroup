{{#if href}}
    {{#unless enabled}}
        <a href="{{{href}}}" class="button-href__no-underlying {{#if messageMenu}}w-full {{/if}}{{#if matchesMenu}}w-full {{/if}}{{#if findCandidateMenu}}w-full {{/if}}{{#if myProfileMenu}}w-full {{/if}}{{#if exitMenu}}w-full {{/if}}">
    {{/unless}}
{{/if}}
    <button class="button-component {{#if enabled}}{{#if messageMenu}}{{/if}}{{#if matchesMenu}}{{/if}}{{#if findCandidateMenu}}button__menu-enabled-red {{/if}}{{#if myProfileMenu}}{{/if}}{{#if exitMenu}}{{/if}}{{/if}}{{#if loginRegisterButton}}login-register-button {{/if}}{{#if messageMenu}}button__menu {{/if}}{{#if matchesMenu}}button__menu {{/if}}{{#if findCandidateMenu}}button__menu {{/if}}{{#if myProfileMenu}}button__menu {{/if}}{{#if exitMenu}}button__menu {{/if}}" type="submit" id={{id}}>
