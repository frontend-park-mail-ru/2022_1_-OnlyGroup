            {{#if messageMenu}}<img alt="icon" class="button-icon" src="/static/images/MsgDark.png">{{/if}}
            {{#if matchesMenu}}<img alt="icon" class="button-icon" src="static/images/cards.png">{{/if}}
            {{#if findCandidateMenu}}<img alt="icon" class="button-icon" src="static/images/RefreshDisable.png">{{/if}}
